  },
  "homepage": "https://github.com/meldify/meldify#readme",
  "keywords": [
    "Meldify",
    "CLI",
    "Templates",
    "Engine"
  ],
  "author": "Meldify",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "commander": "^12.0.0"
  }
}

{
  "name": "meldify",
  "version": "0.0.1",
  "description": "The Meldify CLI allows you to set up projects using Meldify templates",
  "main": "bin/meldify.js",
  "bin": {
    "meldify": "bin/meldify.js"
  },
  "scripts": {
    "build": "npx tsc",
